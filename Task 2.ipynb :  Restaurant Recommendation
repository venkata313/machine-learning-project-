    "from scipy.spatial.distance import pdist, squareform"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 97,
   "id": "23260760-4ecb-468a-a2fe-d372be0f00dc",
   "metadata": {},
   "outputs": [],
   "source": [
    "jaccardDist = pdist(restoXcuisines.values, metric='jaccard')\n",
    "jaccardMatrix = squareform(jaccardDist)\n",
    "jaccardSim = 1 - jaccardMatrix\n",
    "dfJaccard = pd.DataFrame(\n",
    "    jaccardSim,\n",
    "    index=restoXcuisines.index,\n",
    "    columns=restoXcuisines.index)"
   ]
