    "set1 = set(restoXcuisines.loc[\"Olive Bistro\"])\n",
    "set2 = set(restoXcuisines.loc[\"Rose Cafe\"])\n",
    "\n",
    "jaccard_sim = len(set1 & set2) / len(set1 | set2)\n",
    "print(jaccard_sim)"
   ]
